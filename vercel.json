{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node",
            "config": {
                "tsconfigPath": "vercel-tsconfig.json",
                "includeFiles": ["src/*/"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/main.ts",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
        }
    ]
}
